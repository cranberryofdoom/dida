<div class="row new">
	<div class="col-12">
		<form class="form-horizontal">
			<div class="form-group">
				<label for="input-first-name" class="col-lg-2 control-label">First Name</label>
				<div class="col-lg-9">
					<input type="text" class="form-control" id="input-first-name" placeholder="First Name">
				</div>
			</div>
			<div class="form-group">
				<label for="input-last-name" class="col-lg-2 control-label">Last Name</label>
				<div class="col-lg-9">
					<input type="text" class="form-control" id="input-last-name" placeholder="Last Name">
				</div>
			</div>
			<div class="form-group">
				<label for="input-organization" class="col-lg-2 control-label">Organization Name</label>
				<div class="col-lg-9">
					<input type="text" class="form-control" id="input-organization" placeholder="Organization Name">
				</div>
			</div>
			<div class="form-group">
				<label for="input-cell" class="col-lg-2 control-label">Cell Phone Number</label>
				<div class="col-lg-9">
					<input type="text" class="form-control" id="input-cell" placeholder="Cell Phone Number">
				</div>
			</div>
		</form>
	</div>
	<div class="col-6 col-lg-6 pull-right next">
		<h4>Scroll or click to proceed</h4>
		<h3>Next</h3>
	</div>
</div>
<div class="row choose-project">
	<%= render "form/choose" %>
</div>